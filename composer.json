{
    "name": "ouredu/laravel-sqs-messaging",
    "description": "Laravel package for AWS SQS messaging with RabbitMQ rollback support",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "OurEdu Team",
            "email": "dev@oureedu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/console": "^9.0|^10.0",
        "illuminate/database": "^9.0|^10.0",
        "aws/aws-sdk-php": "^3.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "OurEdu\\SqsMessaging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OurEdu\\SqsMessaging\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OurEdu\\SqsMessaging\\SqsMessagingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

